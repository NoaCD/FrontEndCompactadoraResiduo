@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["title"] = "Embarques";
}

@section Styles{
<link rel="stylesheet" type="text/css" href="~/assets/datatables/DataTables-1.11.4/css/jquery.dataTables.css" />
<link rel="stylesheet" type="text/css" href="~/assets/datatables/daterangepicker.css" />
}
<div class="card">
    <div class="container">
        <h2 class="text-center py-3"> Reportes de Embarques </h2>
        <br />
        <div class="float-left">
            <h5>Rangos :</h5>
            <input id="daterange">
        </div>
        <br />
        <br />
        <br />
        <table id="tableReport" class="display" style="width:100%">
            <thead>
                <tr>
                    <th></th>
                    <th>Folio documento</th>
                    <th>Departamento</th>
                    <th>Fecha de creacion</th>
                </tr>
            </thead>
            <tfoot>
                <tr>
                    <th></th>
                    <th>Folio documento</th>
                    <th>Departamento</th>
                    <th>Fecha de creacion</th>
                </tr>
            </tfoot>
        </table>

    </div>
</div>


@section Scripts{

<script src="~/Scripts/EmbarqueController/ListadoDocumentos.js" type="text/javascript"></script>
<script type="text/javascript" src="~/assets/datatables/DataTables-1.11.4/js/jquery.dataTables.js"></script>
<script type="text/javascript" src="~/assets/datatables/daterangepicker.min.js"></script>
<script type="text/javascript" src="~/assets/datatables/moment.min.js"></script>
}
