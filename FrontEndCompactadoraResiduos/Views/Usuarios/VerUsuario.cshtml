@model ItemUsuarioViewModel;
@{
    Layout = "~/Views/Shared/_Modal.cshtml";
}


@section header{
<h1 class="display-4">Editar usuario</h1>
}

<div class="form-group">
    <label for="nombre">Nombre</label>
    <input type="text" class="form-control" id="name" aria-describedby="emailHelp" 
    placeholder="Escribe un nuevo nombre" value="@Model.itemUsuario.cNombre">
    
</div>
<div class="form-group">
    <label for="apellido_paterno"> Apellido paterno  </label>
    <input type="text" class="form-control" id="apellido_materno" 
    placeholder="Cambiar el apellido paterno" value="@Model.itemUsuario.cApellidoPaterno" >
</div>
<div class="form-group">
    <label for="apellido_materno"> Apellido Materno  </label>
    <input type="text" class="form-control" id="apellido_materno" 
    placeholder="Cambiar el apellido materno" value="@Model.itemUsuario.cApellidoMaterno">
</div>
<div class="form-group">
    <label for="apellido_materno"> Fecha de creacion  </label>
    <input type="text" class="form-control" id="apellido_materno" 
    placeholder="Cambiar el apellido materno" value="@Model.itemUsuario.dtFechaCreacion">
</div>






@section footer{
<button class="btn btn-primary" type="button" id="btnGuardar">Guardar</button>
}
